{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/product/(.*)": "<rootDir>/../libs/product/src/$1",
    "@app/product": "<rootDir>/../libs/product/src",
    "@app/shared/(.*)": "<rootDir>/../libs/shared/src/$1",
    "@app/shared": "<rootDir>/../libs/shared/src",
    "@app/providers/(.*)": "<rootDir>/../libs/providers/src/$1",
    "@app/providers": "<rootDir>/../libs/providers/src",
    "@app/entities/(.*)": "<rootDir>/../libs/entities/src/$1",
    "@app/entities": "<rootDir>/../libs/entities/src"
  }
}